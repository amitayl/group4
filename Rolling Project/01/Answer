function User (userName,password,age) {
    this.userName = userName;
    this.password = password;
    this.age = age;
};

function Users ()
{
    this.arrOfUsers = [];

    this.createUser = function (user) {
        // Authentication - dublicated usernames
        for (var index = 0, notFound = true; index < this.arrOfUsers.length && notFound ; index++) {
            if ( this.arrOfUsers[index] === user.userName) {
                console.log ("duplicated user names. User was NOT created");
                notFound = false;
            }
        }
        // check if there was an attempt to duplicate
        if (notFound) {
            this.arrOfUsers.push(user);
        }

    };

    this.deleteUser = function (user) {
        arrOfUsers.splice(this.arrOfUsers.indexOf(user.userName), 0);
    };

    this.getUsersList = function () {
        var arrUsersnames = [];

        for (var index = 0 ; index < this.arrOfUsers.length; index++) {
            arrUsersnames.push(this.arrOfUsers[index]);       
        }
        return arrUsersnames;
    };

};


var user = new User ("Amitay","1234",45);
var users = new Users;
users.createUser(user);
users.getUsersList();

var user2 = new User ("Avi","4321",38);
users.createUser(user2);
users.getUsersList();



